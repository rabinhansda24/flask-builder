{
  "name": "flask-builder",
  "displayName": "Flask Builder",
  "description": "A Visual Studio Code extension to quickly scaffold a Flask application with necessary modules, Docker support, integration and unit tests.",
  "version": "1.1.3",
  "author": {
    "name": "Rabin Hansda",
    "email": "rabinhansda24@live.com"
  },
  "publisher": "RabinHansda",
  "icon": "logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/rabinhansda24/flask-builder"
  },
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Testing"
  ],
  "keywords": [
    "Flask",
    "Python",
    "Web Development",
    "Scaffold",
    "Boilerplate",
    "Docker",
    "Testing",
    "Pytest",
    "MVC",
    "Blueprints",
    "Configuration",
    "VSCode",
    "Extension",
    "Snippets",
    "Backend",
    "API"
  ],
  "pricing": "Free",
  "sponsor": {
    "url": "https://buymeacoffee.com/rabinhansda"
  },
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createPythonModule",
        "title": "Create Python Module"
      },
      {
        "command": "extension.createFlaskApp",
        "title": "Create Flask App"
      },
      {
        "command": "extension.createFlaskModule",
        "title": "Create Flask Module"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.createPythonModule",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        },
        {
          "command": "extension.createFlaskApp",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        },
        {
          "command": "extension.createFlaskModule",
          "when": "explorerResourceIsFolder",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "license": "MIT"
}
